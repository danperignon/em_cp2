{
  "mcpServers": {
    "sequential-thinking": {
      "command": "node",
      "args": ["./servers/sequential-thinking/dist/index.js"],
      "env": {
        "DEBUG": "${DEBUG:-false}",
        "LOG_LEVEL": "${LOG_LEVEL:-info}",
        "THINKING_MODE": "${THINKING_MODE:-adaptive}",
        "MAX_REASONING_DEPTH": "${MAX_REASONING_DEPTH:-5}",
        "PATTERN_LEARNING": "${PATTERN_LEARNING:-true}"
      }
    }
  }
}